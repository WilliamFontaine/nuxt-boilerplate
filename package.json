{
  "name": "nuxt-boilerplate",
  "private": true,
  "type": "module",
  "version": "0.0.0",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "preview": "nuxt preview",
    "lint": "pnpm run lint:prettier && pnpm run lint:eslint",
    "lint:eslint": "eslint . --fix --ignore-pattern volumes/",
    "lint:prettier": "prettier --write --list-different .",
    "postinstall": "nuxt prepare",
    "prepare": "husky",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "tag:patch": "pnpm version patch && git push --follow-tags",
    "tag:minor": "pnpm version minor && git push --follow-tags",
    "tag:major": "pnpm version major && git push --follow-tags"
  },
  "dependencies": {
    "@iconify-json/lucide": "^1.2.57",
    "@nuxt/image": "1.10.0",
    "@nuxt/ui": "3.2.0",
    "@nuxtjs/i18n": "9.5.6",
    "@prisma/nuxt": "^0.3.0",
    "yup": "^1.6.1"
  },
  "devDependencies": {
    "@nuxt/eslint": "1.5.2",
    "eslint": "^9.31.0",
    "husky": "^9.1.7",
    "nuxt": "4.0.0",
    "prettier": "^3.6.2",
    "prisma": "^6.12.0",
    "typescript": "^5.8.3"
  }
}
