{
  "name": "nuxt-boilerplate",
  "private": true,
  "type": "module",
  "version": "1.0.8",
  "author": "William Fontaine",
  "license": "MIT",
  "homepage": "https://github.com/WilliamFontaine/nuxt-boilerplate",
  "repository": {
    "type": "git",
    "url": "https://github.com/WilliamFontaine/nuxt-boilerplate.git"
  },
  "bugs": {
    "url": "https://github.com/WilliamFontaine/nuxt-boilerplate/issues"
  },
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "preview": "nuxt preview",
    "lint": "npm run lint:prettier && npm run lint:eslint",
    "lint:eslint": "eslint . --fix --ignore-pattern volumes/",
    "lint:prettier": "prettier --write --list-different .",
    "postinstall": "nuxt prepare",
    "prepare": "husky",
    "test": "npm run test:unit && npm run test:e2e",
    "test:unit": "vitest run",
    "test:unit:watch": "vitest --watch",
    "test:unit:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "db:seed": "prisma db seed",
    "changelog": "npx git-cliff --output CHANGELOG.md",
    "release:patch": "npm version patch && git push --follow-tags",
    "release:minor": "npm version minor && git push --follow-tags",
    "release:major": "npm version major && git push --follow-tags"
  },
  "dependencies": {
    "@iconify-json/lucide": "^1.2.63",
    "@iconify-json/openmoji": "^1.2.14",
    "@nuxt/image": "1.11.0",
    "@nuxt/ui": "4.0.0-alpha.0",
    "@nuxtjs/i18n": "^10.0.6",
    "@nuxtjs/seo": "^3.1.0",
    "@pinia/nuxt": "^0.11.2",
    "@prisma/nuxt": "^0.3.0",
    "handlebars": "^4.7.8",
    "nodemailer": "^6.9.13",
    "nuxt": "4.0.3",
    "nuxt-auth-utils": "^0.5.23",
    "nuxt-nodemailer": "^1.1.2",
    "nuxt-security": "^2.4.0",
    "pinia": "^3.0.3",
    "pinia-plugin-persistedstate": "^4.5.0",
    "zod": "^4.0.17"
  },
  "devDependencies": {
    "@commitlint/cli": "19.8.1",
    "@commitlint/config-conventional": "19.8.1",
    "@nuxt/eslint": "1.9.0",
    "@nuxt/test-utils": "3.19.2",
    "@playwright/test": "^1.56.1",
    "@types/node": "^24.3.0",
    "@types/swagger-jsdoc": "^6.0.4",
    "@vitest/coverage-v8": "3.2.4",
    "@vue/test-utils": "2.4.6",
    "eslint": "^9.33.0",
    "happy-dom": "18.0.1",
    "husky": "^9.1.7",
    "nuxt-mcp": "^0.2.4",
    "playwright-core": "^1.55.0",
    "prettier": "^3.6.2",
    "prisma": "^6.14.0",
    "swagger-jsdoc": "^6.2.8",
    "tsx": "^4.20.4",
    "typescript": "^5.9.2",
    "vitest": "3.2.4"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "engines": {
    "node": ">=22.0.0",
    "npm": ">=10.0.0"
  }
}
