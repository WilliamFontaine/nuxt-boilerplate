{
  "name": "nuxt-boilerplate",
  "private": true,
  "type": "module",
  "version": "1.1.0",
  "author": "William Fontaine",
  "description": "A Nuxt 4 boilerplate with TypeScript, Pinia, Prisma, i18n, ESLint, Prettier, Vitest, Playwright and more.",
  "license": "MIT",
  "homepage": "https://github.com/WilliamFontaine/nuxt-boilerplate",
  "repository": {
    "type": "git",
    "url": "https://github.com/WilliamFontaine/nuxt-boilerplate.git"
  },
  "bugs": {
    "url": "https://github.com/WilliamFontaine/nuxt-boilerplate/issues"
  },
  "scripts": {
    "prepare": "husky",
    "postinstall": "prisma generate && nuxt prepare",
    "dev": "nuxt dev",
    "build": "nuxt build",
    "preview": "nuxt preview",
    "lint": "pnpm run lint:prettier && pnpm run lint:eslint",
    "lint:eslint": "eslint . --fix",
    "lint:prettier": "prettier --write --list-different .",
    "test": "pnpm run test:unit && pnpm run test:e2e",
    "test:unit": "vitest run",
    "test:unit:watch": "vitest --watch",
    "test:unit:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:seed": "prisma db seed",
    "processor:dev": "nuxt-processor dev",
    "changelog": "pnpm exec git-cliff --output CHANGELOG.md",
    "release:patch": "pnpm version patch && git push --follow-tags",
    "release:minor": "pnpm version minor && git push --follow-tags",
    "release:major": "pnpm version major && git push --follow-tags"
  },
  "dependencies": {
    "@nuxt/image": "1.11.0",
    "@nuxt/ui": "4.0.1",
    "@nuxtjs/i18n": "^10.0.6",
    "@nuxtjs/seo": "^3.1.0",
    "@pinia/nuxt": "^0.11.2",
    "@prisma/client": "6.18.0",
    "handlebars": "^4.7.8",
    "nodemailer": "^7.0.9",
    "nuxt": "4.1.3",
    "nuxt-auth-utils": "^0.5.23",
    "nuxt-security": "^2.4.0",
    "pinia": "^3.0.3",
    "pinia-plugin-persistedstate": "^4.5.0",
    "zod": "^4.0.17"
  },
  "devDependencies": {
    "@commitlint/cli": "20.1.0",
    "@commitlint/config-conventional": "20.0.0",
    "@iconify-json/lucide": "^1.2.63",
    "@iconify-json/openmoji": "^1.2.14",
    "@iconify-json/simple-icons": "^1.2.54",
    "@nuxt/eslint": "1.9.0",
    "@nuxt/test-utils": "3.20.1",
    "@playwright/test": "^1.55.0",
    "@types/node": "^24.3.0",
    "@types/nodemailer": "^7.0.2",
    "@types/swagger-jsdoc": "^6.0.4",
    "@vitest/coverage-v8": "4.0.3",
    "@vue/test-utils": "2.4.6",
    "eslint": "^9.33.0",
    "happy-dom": "20.0.8",
    "husky": "^9.1.7",
    "playwright-core": "^1.55.0",
    "prettier": "^3.6.2",
    "prisma": "^6.14.0",
    "swagger-jsdoc": "^6.2.8",
    "tsx": "^4.20.6",
    "typescript": "^5.9.2",
    "vitest": "4.0.3"
  },
  "packageManager": "pnpm@10.19.0"
}
